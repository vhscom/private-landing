{
	"name": "@private-landing/core",
	"version": "1.0.0",
	"description": "Authentication system for Cloudflare Workers",
	"license": "Apache-2.0",
	"author": "Josh Habdas <josh@comfus.io>",
	"homepage": "https://github.com/vhscom/private-landing",
	"repository": {
		"type": "git",
		"url": "https://github.com/vhscom/private-landing",
		"directory": "packages/core"
	},
	"keywords": [
		"authentication",
		"jwt",
		"sessions",
		"cloudflare-workers",
		"security"
	],
	"type": "module",
	"engines": {
		"node": ">=24.0.0",
		"bun": ">=1.0.0"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		},
		"./auth": {
			"types": "./dist/auth/index.d.ts",
			"import": "./dist/auth/index.js"
		},
		"./config": {
			"types": "./dist/auth/config/index.d.ts",
			"import": "./dist/auth/config/index.js"
		}
	},
	"files": ["dist"],
	"scripts": {
		"clean": "rm -rf dist tsconfig.tsbuildinfo",
		"build": "tsc --build",
		"typecheck": "tsc --noEmit"
	},
	"dependencies": {
		"@libsql/client": "^0.15.0",
		"@private-landing/infrastructure": "workspace:*",
		"@private-landing/schemas": "workspace:*",
		"@private-landing/types": "workspace:*",
		"hono": "^4.7.5",
		"nanoid": "^5.1.5"
	},
	"devDependencies": {
		"bun-types": "^1.2.5",
		"typescript": "^5.0.0"
	}
}
