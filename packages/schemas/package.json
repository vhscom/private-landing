{
	"name": "@private-landing/schemas",
	"version": "1.0.0",
	"description": "Zod validation schemas for authentication system",
	"license": "Apache-2.0",
	"author": "Josh Habdas <josh@comfus.io>",
	"homepage": "https://github.com/vhscom/private-landing/tree/archive/cloudflare-launchpad",
	"repository": {
		"type": "git",
		"url": "https://github.com/vhscom/private-landing",
		"directory": "packages/schemas"
	},
	"keywords": ["validation", "schemas", "zod", "typescript"],
	"type": "module",
	"engines": {
		"node": ">=18.0.0"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		},
		"./auth": {
			"types": "./dist/auth/index.d.ts",
			"import": "./dist/auth/index.js"
		},
		"./utils": {
			"types": "./dist/utils/index.d.ts",
			"import": "./dist/utils/index.js"
		}
	},
	"files": ["dist"],
	"scripts": {
		"clean": "rm -rf dist tsconfig.tsbuildinfo",
		"build": "tsc --build",
		"typecheck": "tsc --noEmit"
	},
	"dependencies": {
		"zod": "^4.3.5"
	},
	"devDependencies": {
		"bun-types": "^1.2.5",
		"typescript": "^5.0.0"
	}
}
