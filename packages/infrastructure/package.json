{
	"name": "@private-landing/infrastructure",
	"version": "1.0.0",
	"description": "Database and middleware utilities for Cloudflare Workers",
	"license": "Apache-2.0",
	"author": "Josh Habdas <josh@comfus.io>",
	"homepage": "https://github.com/vhscom/private-landing/tree/archive/cloudflare-launchpad",
	"repository": {
		"type": "git",
		"url": "https://github.com/vhscom/private-landing",
		"directory": "packages/infrastructure"
	},
	"keywords": ["infrastructure", "database", "middleware", "cloudflare-workers"],
	"type": "module",
	"engines": {
		"node": ">=18.0.0"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		},
		"./db": {
			"types": "./dist/db/index.d.ts",
			"import": "./dist/db/index.js"
		},
		"./middleware": {
			"types": "./dist/middleware/index.d.ts",
			"import": "./dist/middleware/index.js"
		}
	},
	"files": ["dist"],
	"scripts": {
		"clean": "rm -rf dist tsconfig.tsbuildinfo",
		"build": "tsc --build",
		"typecheck": "tsc --noEmit"
	},
	"dependencies": {
		"@libsql/client": "^0.15.0",
		"@private-landing/types": "workspace:*"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "4.20250224.0",
		"bun-types": "^1.2.5",
		"typescript": "^5.0.0"
	}
}
