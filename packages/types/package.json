{
	"name": "@private-landing/types",
	"version": "1.0.0",
	"description": "Shared TypeScript types for authentication system",
	"license": "Apache-2.0",
	"author": "Josh Habdas <josh@comfus.io>",
	"homepage": "https://github.com/vhscom/private-landing",
	"repository": {
		"type": "git",
		"url": "https://github.com/vhscom/private-landing",
		"directory": "packages/types"
	},
	"keywords": ["types", "typescript", "authentication"],
	"type": "module",
	"engines": {
		"node": ">=24.0.0",
		"bun": ">=1.0.0"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		},
		"./auth": {
			"types": "./dist/auth/index.d.ts",
			"import": "./dist/auth/index.js"
		},
		"./http": {
			"types": "./dist/http/index.d.ts",
			"import": "./dist/http/index.js"
		},
		"./env": {
			"types": "./dist/env.d.ts",
			"import": "./dist/env.js"
		}
	},
	"files": ["dist"],
	"scripts": {
		"clean": "rm -rf dist tsconfig.tsbuildinfo",
		"build": "tsc --build",
		"typecheck": "tsc --noEmit"
	},
	"dependencies": {
		"hono": "^4.7.5"
	},
	"devDependencies": {
		"bun-types": "latest",
		"typescript": "^5.0.0"
	}
}
